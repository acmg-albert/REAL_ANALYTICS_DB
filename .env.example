# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=real_estate_analytics
DB_USER=your_username
DB_PASSWORD=your_password

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# API Configuration
APARTMENT_LIST_API_KEY=your_api_key
APARTMENT_LIST_API_BASE_URL=https://www.apartmentlist.com/api/v2

# Scraping Configuration
SCRAPING_DELAY=2.0
MAX_RETRIES=3
TIMEOUT=30
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

# Data Source Configuration
APARTMENTLIST_BASE_URL=https://www.apartmentlist.com/research/category/data-rent-estimates

# File Paths
DOWNLOAD_PATH=data/raw
DATA_PATH=data
ARCHIVE_PATH=data/archive

# Data Retention (in days)
DATA_RETENTION_DAYS=7
ARCHIVE_RETENTION_DAYS=60

# Data Validation
MIN_RENT_VALUE=0
VALIDATE_DATE_CONTINUITY=true
VALIDATE_LOCATION_CODES=true

# Email Notification (Optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password
NOTIFICATION_EMAIL=your_email@gmail.com 